#include <iostream>
#include <ctime>

using namespace std;

int main()
{
	const int length = 5;



	int arr[length] = {};

	for (int i = 0; i < length; i++)

	{
		arr[i] = rand() % 20 + 1;
		cout << arr[i] << endl;
	}

	cout << "\n";

	cout << "reversed array" << endl;

	for (int i = length - 1; i >= 0; i--)
	{
		cout << arr[i] << endl;
		
	}

	system("pause");
	
}
